func factorial_iteration(n : real) -> real {
	result : real;
	index : real;

	result = 1;
	index = 0;
	
	while n > 1 {
		result = result * n;
		n = n - 1;
	}

	return result;
}	

func factorial_recursive(n : real) -> real {
	if n == 1 {
		return 1;
	}
	return n * factorial_recursive(n - 1);
}

func main() -> null {
	value : real;

	value = 5;
	
	println("iterative factorial of %d calculated to be: %d", value, factorial_iteration(value));
	println("recursive factorial of %d calculated to be: %d", value, factorial_recursive(value));
}






