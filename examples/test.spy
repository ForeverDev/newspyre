func factorial_iteration(n : real) -> real {
	result : real;
	index : real;

	result = 1;
	index = 0;
	
	while n > 1 {
		result = result * n;
		n = n - 1;
	}

	return result;
}	

func factorial_recursive(n : real) -> real {
	if n == 1 {
		return 1;
	}
	return n * factorial_recursive(n - 1);
}

func print_grid(x_dimension : real, y_dimension : real) -> null {
	start_x : real;

	start_x = x_dimension;

	while y_dimension > 0 {
		while x_dimension > 0 {
			print("X ");
			x_dimension = x_dimension - 1;
		}
		print("\n");
		y_dimension = y_dimension - 1;
		x_dimension = start_x;
	}
}

func main() -> null {
	input : real;

	input = 5;
	
	println("iterative factorial of %d calculated to be: %d", input, factorial_iteration(input));
	println("recursive factorial of %d calculated to be: %d", input, factorial_recursive(input));

	print_grid(5, 10);
}






